{
    "filter_paths": "lib, lib/ERC721A/contracts/ERC721A.sol, lib/openzeppelin-contracts/, contracts/Marketplace.sol, contracts/LandBank.sol",
    "detectors_to_exclude": "timestamp,naming-convention,pragma,solc-version,uninitialized-local,divide-before-multiply,unchecked-transfer,constable-states,unused-return",
    "solc_remaps": [
        "forge-std/=lib/forge-std/src/",
        "@openzeppelin/=lib/openzeppelin-contracts/",
        "@rari-capital/solmate/=lib/solmate/",
        "@uniswap/v2-core/=lib/v2-core/",
        "@uniswap/v2-periphery/=lib/v2-periphery/",
        "ERC721A/=lib/ERC721A/"
    ]
}